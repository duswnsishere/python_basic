a,b = input().split()
c = input()
a = int(a)
b = int(b)
c = int(c)
min_sum = b + c
hour_add = min_sum // 60 
final_min = min_sum % 60
final_hour = a + hour_add
if final_hour >= 24:
    print_hour = final_hour % 24
    print(print_hour,final_min,sep = ' ')
else:
    print(final_hour,final_min,sep = " ")
