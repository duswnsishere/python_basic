a, b, c = input().split()
a = int(a)
b = int(b)
c = int(c)
if a == b == c:
    10000 + a*1000
elif a == b:
    print(1000 + a*100)
elif a == c:
    print(1000 + a*100)
elif b == c:
    print(1000 + b*100)
elif a != b != c:
    d = ((a if a>b else b) if ((a if a>b else b)>c) else c)
    print(100*d)

